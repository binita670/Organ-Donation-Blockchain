// Navbar
nav.navbar.fixed-top.navbar-expand-lg.blue-gradient(style='color: white;')
  // Breadcrumb
  .breadcrumb-dn.mr-auto(style='display: flex;')
    img(src='/images/donar.jpg' height='52px;' width='52px;')
    a(href='/' style='margin-left: 20px; font-size: 30px; color:white;' ) Organ Donation
  ul.nav.navbar-nav.nav-flex-icons.ml-auto
    -if(!donor)
      li.nav-item
        a.nav-link
          i.fas.fa-envelope
          span.clearfix.d-none.d-sm-inline-block Contact
      li.nav-item
        a.nav-link
          i.far.fa-comments
          span.clearfix.d-none.d-sm-inline-block Support
      li.nav-item.dropdown
        a#navbarDropdownMenuLink.nav-link.dropdown-toggle(data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
          i.fas.fa-user
          |             Account
        .dropdown-menu.dropdown-menu-right(aria-labelledby='navbarDropdownMenuLink')
          a.dropdown-item(href='/donor/register') Signup
          a.dropdown-item(href='/donor') Signin
    -else
      li.nav-item.dropdown
        a#navbarDropdownMenuLink.nav-link.dropdown-toggle(data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
          i.fas.fa-user
          |             Account
        .dropdown-menu.dropdown-menu-right(aria-labelledby='navbarDropdownMenuLink')
          a.dropdown-item(href='/donor/logout') Signout
        
          
